# GenAI (LLM) module environment variables
# Copy to genai_module/.env (local, do not commit)

# Provider selection and general model settings
LLM_PROVIDER=groq
LLM_MODEL=llama-3.3-70b-versatile
LLM_TEMPERATURE=0.3
MAX_TOKENS=2000

# Provider-specific keys (set only the ones you use)
# GROQ
GROQ_API_KEY=your_groq_api_key_here
GROQ_API_KEY_2=
GROQ_API_KEY_3=
# OpenAI
OPENAI_API_KEY=
# Azure
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
# Hugging Face
HUGGINGFACE_API_KEY=
# Together AI
TOGETHER_API_KEY=
# Ollama (local)
OLLAMA_BASE_URL=http://localhost:11434
